algoritmo "ex038"

// Disciplina   : [Linguagem e Lógica de Programação]
// Professor   : Gustavo Guanabara - Cursoemvideo
// Descrição   : De 6 até 11
// Autor(a)    : Lih Yasumura

var

  N: Inteiro

inicio

  N <- 6
  Enquanto (N <= 11) faca
      Escreva (N )
      N <- N + 1
  FimEnquanto
  
  EscrevaL (" -> Acabou!")

fimalgoritmo