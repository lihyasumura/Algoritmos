algoritmo "ex041"

// Disciplina   : [Linguagem e Lógica de Programação]
// Professor   : Gustavo Guanabara - Cursoemvideo
// Descrição   : De 5 em 5 decrescente
// Autor(a)    : Lih Yasumura

var

  N: Inteiro

inicio

  N <- 100
  Enquanto (N >= 0) faca
      Escreva (N )
      N <- N - 5
  FimEnquanto
  EscrevaL (" -> Acabou!")

fimalgoritmo