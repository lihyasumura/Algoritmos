algoritmo "ex039"

// Disciplina   : [Linguagem e Lógica de Programação]
// Professor   : Gustavo Guanabara - Cursoemvideo
// Descrição   : De 10 até 3
// Autor(a)    : Lih Yasumura

var

  N: Inteiro

inicio

  N <- 10
  Enquanto (N >= 3) faca
      Escreva (N )
      N <- N - 1
  FimEnquanto
  
  EscrevaL (" -> Acabou!")

fimalgoritmo